{
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
    }
  },
  "rules": {
    "valid-jsdoc": 2,
    "curly": 2,
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 2,
    "react/wrap-multilines": 2,
    "comma-dangle": ["error", "never"]
  },
  "plugins": [
    "react"
  ]
}
